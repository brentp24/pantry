<div id="navbarBasicExample" class="navbar-menu">
    <div id="brand">
        <img src="assets/img/pantree_primary_logo.png" alt="pantree_brand">
        <!--swap in pantree logo -->
    </div>
    <div class="navbar-start">
        <a class="navbar-item">
            Main Pantry
        </a>

        <a class="navbar-item">
            Shopping List
        </a>

        <div class="navbar-item has-dropdown is-hoverable">
            <a class="navbar-link">
                Meals
            </a>

            <div class="navbar-dropdown">
                <a class="navbar-item">
                    Breakfast
                </a>
                <a class="navbar-item">
                    Lunch
                </a>
                <a class="navbar-item">
                    Dinner
                </a>
                <a class="navbar-item">
